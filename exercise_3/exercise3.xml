<suite name="exercise3" company="testwarez2016" project="workshops">
    <test name="close-cookie-disclaimer">
        <collect>
            <open/>
            <wait-for-page-loaded/>
            <resolution width="1200" height="800"/>
            <sleep duration="2000"/>
            <screen name="desktop"/>
        </collect>
        <compare>
            <screen comparator="layout"/>
        </compare>
        <urls>
            <url href="/contact.html"/>
        </urls>
    </test>
